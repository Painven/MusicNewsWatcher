<Application x:Class="MusicNewsWatcher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:con="clr-namespace:MusicNewsWatcher.Infrastructure.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Animations.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Brushes.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Converters.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Buttons.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
